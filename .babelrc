{
  "env": {
    "development": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ],
      "plugins": [
        "add-module-exports",
        "@babel/plugin-proposal-class-properties"
      ]
    },
    "production": {
      "presets": [
        "@babel/preset-env",
        "minify",
        "@babel/preset-react"
      ],
      "plugins": [
        "add-module-exports",
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
